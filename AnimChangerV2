--Properties:

ScreenGui.Parent = game.CoreGui

main.Name = "main"
main.Parent = ScreenGui
main.BackgroundColor3 = Color3.fromRGB(49, 44, 44)
main.Position = UDim2.new(0.681349576, 0, 0.485089451, 0)
main.Size = UDim2.new(0, 340, 0, 259)

label.Name = "label"
label.Parent = main
